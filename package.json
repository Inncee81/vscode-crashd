{
	"name": "crashd",
	"displayName": "Zelos CrasHD",
	"description": "Highlight source files & visualize crash information generated by Zelos.",
	"version": "0.1.0",
	"license": "MIT",
	"publisher": "zeropoint",
	"repository": {
		"type": "git",
		"url": "https://github.com/zeropointdynamics/vscode-crashd.git"
	},
	"author": {
		"name": "Ryan Court",
		"email": "rccourt@zeropointdynamics.com"
	},
	"contributors": [
		{
			"name": "Kevin Snow",
			"email": "kevin@zeropointdynamics.com"
		}
	],
	"activationEvents": [
		"onCommand:crashd.show",
		"onCommand:crashd.hide",
		"onCommand:crashd.reloadZcovFiles"
	],
	"engines": {
		"vscode": "^1.46.0"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "crashd.show",
				"title": "Show",
				"category": "CrasHD"
			},
			{
				"command": "crashd.hide",
				"title": "Hide",
				"category": "CrasHD"
			},
			{
				"command": "crashd.reloadZcovFiles",
				"title": "Reload (Load new .zcov File)",
				"category": "CrasHD"
			}
		],
		"configuration": []
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {},
	"__metadata": {
		"id": "bf93bc76-53d0-4c2c-9613-cc8199ac98c3",
		"publisherDisplayName": "Zeropoint Dynamics",
		"publisherId": "7b82ecfb-db7c-439b-b573-a96c1263c084"
	}
}
